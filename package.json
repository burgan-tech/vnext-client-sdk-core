{
  "name": "vnext-client-sdk-core",
  "version": "0.1.0",
  "private": true,
  "description": "Low-code platform client SDKs - TypeScript and Flutter core SDKs with Vue adapter",
  "workspaces": [
    "core/*",
    "adapters/*",
    "examples/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "clean": "npm run clean --workspaces",
    "test": "npm run test --workspaces",
    "lint": "npm run lint --workspaces",
    "mock:server": "node mocks/server.js"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "msw": "^2.0.0",
    "express": "^4.18.2",
    "@types/express": "^4.17.21"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}


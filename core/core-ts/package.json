{
  "name": "@vnext/core-ts",
  "version": "0.1.0",
  "description": "TypeScript core SDK for VNext low-code platform",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.mjs",
      "require": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "dev": "tsup --watch"
  },
  "keywords": [
    "vnext",
    "sdk",
    "typescript",
    "low-code"
  ],
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {},
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}

